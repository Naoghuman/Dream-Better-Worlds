<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="330.0" prefWidth="495.0" style="-fx-background-color: lightyellow;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.pro.dbw.feature.voting.impl.votingwizard.VotingWizardPresenter">
   <children>
      <HBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox maxWidth="-Infinity" minWidth="-Infinity" prefWidth="230.0" HBox.hgrow="NEVER">
               <children>
                  <Label text="Active Votings" />
                  <ListView fx:id="lvActiveVotings" VBox.vgrow="ALWAYS" />
                  <HBox alignment="TOP_RIGHT" spacing="10.0">
                     <children>
                        <Button fx:id="bRemove" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onActionRemove" prefWidth="64.0" text="Remove" />
                        <Button fx:id="bAdd" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onActionAdd" prefWidth="64.0" text="Add" />
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
            <VBox maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Available Votings">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <HBox spacing="10.0">
                     <children>
                        <ComboBox fx:id="cbAvailableVotings" maxWidth="1.7976931348623157E308" prefWidth="150.0" HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" onAction="#onActionShowVotingEditor" text="..." />
                     </children>
                  </HBox>
                  <Label text="Active during" />
                  <HBox alignment="CENTER_LEFT" spacing="2.0">
                     <children>
                        <DatePicker fx:id="dpFrom" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="104.0" />
                        <Label text="-" />
                        <DatePicker fx:id="dpTo" editable="false" layoutX="10.0" layoutY="10.0" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="104.0" />
                     </children>
                  </HBox>
                  <Pane VBox.vgrow="ALWAYS" />
                  <HBox alignment="TOP_RIGHT" spacing="10.0">
                     <children>
                        <Button fx:id="bSave" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onActionSave" prefWidth="64.0" text="Save" />
                        <Button maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onActionClose" prefWidth="64.0" text="Close" />
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>

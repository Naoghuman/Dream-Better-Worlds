                        <HBox alignment="TOP_RIGHT">
                           <children>
                              <Hyperlink text="read more..." />
                           </children>
                        </HBox>
                    </children>
                </VBox>
            </content>
            <graphic>
                <Button fx:id="bEdit" mnemonicParsing="false" onAction="#onActionEdit" text="E" />
            </graphic>
        </TitledPane>
    </children>
</AnchorPane>

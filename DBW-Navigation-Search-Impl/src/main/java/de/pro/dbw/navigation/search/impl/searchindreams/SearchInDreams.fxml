<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" minHeight="300.0" minWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.pro.dbw.navigation.search.impl.searchindreams.SearchInDreamsPresenter">
   <children>
      <VBox spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <ScrollPane fx:id="spSearchComponents" hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <content>
                  <VBox fx:id="vbSearchComponents" spacing="5.0" />
               </content>
            </ScrollPane>
            <VBox fx:id="vbSimpleSqlInfo" />
            <HBox alignment="TOP_RIGHT">
               <children>
                  <Button fx:id="bSearchInDreams" mnemonicParsing="false" onAction="#onActionSearchInDreams" text="Search" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
